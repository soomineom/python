t = int(input())
result = []

for _ in range(t):
    list = []
    n = int(input())
    for i in range(1,n,1):
        for j in range(i,n,1):
            if(i !=j and i<j and n == i + j):
                list.append('{} {}'.format(i,j))
    #result.append(list)
    #print('Pairs for {}: {}'.format(n, ','.join(list)))
    result.append('Pairs for {}: {}'.format(n, ', ' .join(list)))

for k in range(t):
    print(result[k])
